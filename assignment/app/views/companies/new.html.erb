<% if @company.errors.any? %>
	<% @company.errors.full_messages.each do |x| %>
	<%=x%>
	<%end%>
<%end%>

<%=form_for @company  do |f| %>

Name: <%= f.text_field :name %>
 <%= f.fields_for :addr , @company.address do |addr| %>
