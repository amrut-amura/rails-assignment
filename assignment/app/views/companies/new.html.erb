<br><br><br>

<%end%>
<%= f.email_field :email, placeholder: "Your email id" %>
<%= f.password_field :password ,placeholder: "Enter your password"%>
<%= f.password_field :password_confirmation ,placeholder: "Re Enter your password"%>
    <ul id="myList">
    </ul>
 <%= submit_tag "Add address", :type => 'button', :id => 'addAddress', :onclick => 'addAddressField()' %> 
<%= f.submit "submit" %>
<%end%>
